    <script>
        function pickRandomItem() {
            const ul = document.getElementById('itemList');
            const items = Array.from(ul.children);
            const pickedItemsDiv = document.getElementById('pickedItems');

            if (items.length < 1) {
                alert('Not enough items to pick randomly.');
                return;
            }

            // Pick one random item
            const randomIndex = Math.floor(Math.random() * items.length);
            const pickedItem = items[randomIndex].innerText;

            // Remove picked item from the list
            ul.removeChild(items[randomIndex]);

            // Display picked item
            pickedItemsDiv.innerHTML = `
                <h3>Picked Item:</h3>
                <ul><li>${pickedItem}</li></ul>
            `;
        }
    </script>
